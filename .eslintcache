ion changeLisi (name) {\n    console.log('李四来了', name);\n    return name + '李四来了！！！';\n  }\n\n  // 解决不必要的渲染，优化性能\n  // 第二个参数，name变化时，才重新渲染\n  const actionLisi = useCallback(() => {\n    return changeLisi(name)\n  }, [name]);\n\n  return (\n    <>\n      <div>{actionLisi()}</div>\n      { /* 王五更新，李四每次也会打印，性能影响 */}\n      {/* <div>{changeLisi()}</div> */}\n      <div>{children}</div>\n    </>\n  );\n}\n\nexport default UseCallbackDemo;","/Users/wuhuijie/github/learn-react/src/hooks/useLayoutEffect/useLayoutEffectDemo.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":1,"column":27,"nodeType":"97","messageId":"98","endLine":1,"endColumn":34},"no-native-reassign",["99"],"no-negated-in-lhs",["100"],"no-unused-vars","'useMemo' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]